{
  "name": "mx-icons",
  "version": "1.0.5",
  "description": "Beautiful hand-crafted SVG icons for React - light mode only",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./Icon": {
      "import": "./dist/Icon.js",
      "require": "./dist/Icon.cjs"
    },
    "./*": {
      "import": "./dist/*/index.js",
      "require": "./dist/*/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "icons",
    "svg",
    "components",
    "mx-icons",
    "react-icons"
  ],
  "author": "Manish kumar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ig-imanish/mx-icons"
  },
  "homepage": "https://github.com/ig-imanish/mx-icons#readme",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build --config vite.lib.config.js",
    "generate": "node scripts/generate-icons.js",
    "prepublishOnly": "npm run generate && npm run build:lib",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "vite": "^7.1.7"
  }
}
