{
  "name": "mx-icons",
  "version": "1.0.23",
  "description": "Beautiful hand-crafted SVG icons for React - light mode only",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./Icon": {
      "import": "./dist/Icon.js"
    },
    "./*": {
      "import": "./dist/*/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "icons",
    "svg",
    "components",
    "mx-icons",
    "react-icons"
  ],
  "author": "Manish kumar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ig-imanish/mx-icons"
  },
  "homepage": "https://github.com/ig-imanish/mx-icons#readme",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "npm run generate:types && vite build --config vite.lib.config.js && npm run copy:types",
    "generate:types": "node scripts/generate-types.js",
    "copy:types": "node -e \"const fs=require('fs'); fs.copyFileSync('src/icons/index.d.ts','dist/index.d.ts'); if(fs.existsSync('src/icons/Icon.d.ts')){fs.copyFileSync('src/icons/Icon.d.ts','dist/Icon.d.ts')}\"",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "react-dom": "^18.0.0",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "react-router-dom": "^7.12.0"
  }
}
